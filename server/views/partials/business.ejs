<%- include partials/header.ejs%>
<%- include partials/main_nav.ejs%>
    <!--Main Section-->
    <body>
        <main class="container">
            <div class="row">
                <div class="col-md-offset-3 col-md-6">
                <h1><%=title%></title></h1>
                <br>
                <table class = "table table-bordered table-striped table-hover" style="width: 700px;">
                    <tr>
                        <th class="text-center">Contact Name</th>
                        <th class="text-center">Email Address</th>
                        <th class="text-center">Contact Number</th>
                    </tr>
                    <!--Repeatable Template Row-->
                    <% for(let count =0; count<BusinessList.length; count++){ %>
                        <tr>
                            <th class="text-center"><%= BusinessList[count].name %> </th>
                            <th class="text-center"><%= BusinessList[count].email %> </th>
                            <th class="text-center"><%= BusinessList[count].contactnumber %> </th>
                            <th class="text-center"><button onclick="Update()">Update</button></th>
                            <th class="text-center"><button>Delete</button></th>
                        </tr>
                        <% } %>
                </table>
            </div>
            </div>
        </main>
    </body>
<%- include partials/bottom_nav.ejs%>
<%- include partials/footer.ejs%>